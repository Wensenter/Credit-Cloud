.video_mask_box
  position absolute
  top 0
  left 0
  width 100vw
  height 100vh
  background-color: rgba(1,1,1,.8)
  z-index 99
  display none
  .video_play_box
    position relative
    top 0
    left 0
    width 100%
    height 100%
    display flex
    justify-content center
    align-items center
    video 
      max-width 94%
      max-height 90%